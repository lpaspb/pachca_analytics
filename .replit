modules = ["nodejs-20", "web"]
run = "npm run dev"

[nix]
channel = "stable-24_05"

[deployment]
run = ["sh", "-c", "npm run preview -- --host 0.0.0.0 --port 8080 --https"]
deploymentTarget = "cloudrun"
build = ["sh", "-c", "npm run build"]

[workflows]
runButton = "Dev"

[[workflows.workflow]]
name = "Dev"
author = 40559688
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"

[[ports]]
localPort = 8080
externalPort = 80
